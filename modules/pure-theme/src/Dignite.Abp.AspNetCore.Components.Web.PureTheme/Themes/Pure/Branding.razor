@using Volo.Abp.Ui.Branding
@inject IBrandingProvider BrandingProvider

<BarBrand Class="main-navbar-brand">
    @if (!BrandingProvider.LogoReverseUrl.IsNullOrEmpty())
    {
        <img src="@BrandingProvider.LogoReverseUrl" class="navbar-brand-logo" title="@BrandingProvider.AppName" />
    }
    @BrandingProvider.AppName
</BarBrand>

